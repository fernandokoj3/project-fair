{
  "name": "project-fair-api",
  "version": "1.0.0",
  "description": "Fair project ",
  "main": "handler.js",
  "scripts": {
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "start": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/handler.ts",
    "start:local": "set ENVIRONMENT=local&& ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/handler.ts",
    "seed": "set ENVIRONMENT=local&& ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed --dataSource ./src/app/database/data-source.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir .build --copy-files",
    "prepare": "husky install",
    "test": "jest",
    "test:integration": "jest --testPathPattern=src/test/integration",
    "test:unit": "jest --testPathPattern=src/app/",
    "test:unit:utils": "jest --testPathPattern=src/app/utils",
    "test:unit:dto": "jest --testPathPattern=src/app/dto",
    "test:unit:service": "jest --testPathPattern=src/app/services",
    "lint": "npx eslint ./src --fix",
    "pre-commit": "lint-staged",
    "yalc:install": "npm i -g yalc",
    "migration:show:local": "set ENVIRONMENT=local&& npm run typeorm migration:show",
    "migration:run:local": "set ENVIRONMENT=local&& npx typeorm-ts-node-commonjs -d ./src/app/database/data-source.ts migration:run ",
    "migration:revert:local": "set ENVIRONMENT=local&& npm run typeorm migration:revert",
    "migration:generate": "set ENVIRONMENT=local&& ts-node node_modules/typeorm/cli.js migration:generate -n"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fernandokoj3/project-fair.git"
  },
  "keywords": [
    "Fair"
  ],
  "author": "Luis Fernando Camargo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fernandokoj3/project-fair/issues"
  },
  "homepage": "https://github.com/fernandokoj3/project-fair#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/node": "^7.19.1",
    "@babel/plugin-proposal-decorators": "^7.19.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@serverless/typescript": "^3.21.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "better-sqlite3": "^7.6.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "i": "^0.3.7",
    "jest": "^29.2.1",
    "jest-when": "^3.5.1",
    "nock": "^13.2.9",
    "npm": "^8.19.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "serverless-offline": "^11.1.3",
    "serverless-plugin-typescript": "^2.1.4",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "class-validator-extended": "^1.1.0",
    "csv": "^6.2.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.4",
    "json-bigint": "^1.0.0",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "log4js": "^6.7.0",
    "pg": "^8.8.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.1.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.10",
    "typeorm-extension": "^2.1.15",
    "typeorm-typedi-extensions": "^0.4.1",
    "utility-types": "^3.10.0",
    "uuid": "^9.0.0"
  }
}
